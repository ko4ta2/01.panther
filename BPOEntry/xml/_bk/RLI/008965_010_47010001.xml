<?xml version="1.0" encoding="utf-8"?>
<EntryItemRegions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <!--
    [Usage]
      Region.OriginX : 表示領域の原点(X座標)
      Region.OriginY : 表示領域の原点(Y座標)
      Region.Name    : その領域に含むコントロール名
  -->
  
  <!-- 左上領域 -->
  <Region OriginX="25"
          OriginY="0">
    <Name>text001</Name>
  </Region>
  
  <!-- 左下領域 -->
  <Region OriginX="25"
          OriginY="455">
  </Region>
  
  <!-- 右上領域 -->
  <Region OriginX="1075"
          OriginY="0">
  </Region>
  
  <!-- 右下領域 -->
  <Region OriginX="1075"
          OriginY="455">
  </Region>
</EntryItemRegions>